{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/nginx/access.log",
            "log_group_name": "/wordpress/nginx/access",
            "log_stream_name": "{instance_id}-access"
          },
          {
            "file_path": "/var/log/nginx/error.log",
            "log_group_name": "/wordpress/nginx/error",
            "log_stream_name": "{instance_id}-error"
          },
          {
            "file_path": "/var/log/nginx/dev-tf-techbull.com.access.log",
            "log_group_name": "/wordpress/nginx/dev-access",
            "log_stream_name": "{instance_id}-dev-access"
          },
          {
            "file_path": "/var/log/nginx/dev-tf-techbull.com.error.log",
            "log_group_name": "/wordpress/nginx/dev-error",
            "log_stream_name": "{instance_id}-dev-error"
          },
          {
            "file_path": "/var/log/php-fpm/error.log",
            "log_group_name": "/wordpress/php-fpm/error",
            "log_stream_name": "{instance_id}-php-fpm"
          }
        ]
      }
    },
    "log_stream_name": "default"
  }
}